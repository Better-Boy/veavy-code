{
	"name": "weavy",
	"displayName": "veavy's code",
	"description": "Weavy VSCode Extension",
	"icon": "images/logo.v1.png",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/Better-Boy/veavy-code"
	},
	"publisher": "Abhilash",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:weavy.messenger",
		"onCommand:weavy.files",
		"onCommand:weavy.posts",
		"onCommand:weavy.tasks"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "weavy.messenger",
				"title": "Weavy Messenger"
			},
			{
				"command": "weavy.files",
				"title": "Weavy Files"
			},
			{
				"command": "weavy.posts",
				"title": "Weavy Posts"
			},
			{
				"command": "weavy.tasks",
				"title": "Weavy Tasks"
			}
		],
		"configuration": [
			{
				"title": "Weavy",
				"properties": {
					"weavy.url": {
						"type": "string",
						"description": "Weavy EndPoint Url",
						"pattern": "^(http|https)://.*",
						"examples": ["http://localhost:44323", "https://localhost:44323/"],
						"scope": "application"
					},
					"weavy.user.mail": {
						"type": "string",
						"description": "Weavy User Mail",
						"examples":["dave@weavy.com"],
						"scope": "application",
						"default": ""
					},
					"weavy.space.key": {
						"type": "string",
						"description": "Weavy Space Key",
						"scope": "application",
						"default": ""
					},
					"weavy.messenger.key": {
						"type": "string",
						"description": "Weavy Messenger Key",
						"scope": "application",
						"default": ""
					},
					"weavy.files.key": {
						"type": "string",
						"description": "Weavy Files Key",
						"scope": "application",
						"default": ""
					},
					"weavy.posts.key": {
						"type": "string",
						"description": "Weavy Posts Key",
						"scope": "application",
						"default": ""
					},
					"weavy.tasks.key": {
						"type": "string",
						"description": "Weavy Tasks Key",
						"scope": "application",
						"default": ""
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
